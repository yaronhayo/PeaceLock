{
  "version": 2,
  "builds": [
    {
      "src": "api/bookings.ts",
      "use": "@vercel/node"
    },
    {
      "src": "dist/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/bookings",
      "dest": "/api/bookings"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}